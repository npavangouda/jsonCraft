{
  "name": "jsonCraft",
  "displayName": "jsonCraft",
  "description": "jsonCraft: A simple tool to format JSON data and replace encoded entities like &nbsp; with proper double quotes.",
  "version": "1.0.1",
  "icon": "assets/jsonCraft.png",
  "publisher": "npavangouda",
  "author": {
    "name": "Pavan N Gouda"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/npavangouda/jsonCraft"
  },
  "type": "commonjs",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jsonCraft.formatJson",
        "title": "jsonCraft: Format JSON"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "jsonCraft.formatJson",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "he": "^1.2.0",
    "parse-json": "^8.1.0"
  }
}